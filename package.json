{
  "name": "juun",
  "version": "0.0.2",
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "private": true,
  "author": "juun",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "build:docker": "docker build --secret id=env,src=.env -t juun .",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:e2e": "pnpm --filter @juun/web exec playwright test",
    "lint-staged": "lint-staged",
    "check-types": "turbo check-types",
    "web": "pnpm --filter @juun/web",
    "api": "pnpm --filter @juun/api",
    "ui": "pnpm --filter @juun/ui",
    "db": "pnpm --filter @juun/db"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "lucide-react": "^0.552.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "sonner": "^2.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.1.0",
    "@commitlint/format": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@config/eslint": "workspace:^",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/coverage-v8": "^4.0.6",
    "commitizen": "^4.3.1",
    "eslint": "^9.39.0",
    "husky": "^9.1.7",
    "inquirer": "~9.3.8",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "storybook": "^10.0.2",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "pnpm": {
    "overrides": {
      "refractor>prismjs": "^1.30.0",
      "tmp": "^0.2.4",
      "vite": "^7.1.11"
