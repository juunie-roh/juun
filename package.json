{
  "name": "juun",
  "version": "0.0.2",
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8",
  "private": true,
  "author": "juun",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "build:docker": "docker build --secret id=env,src=.env -t juun .",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:e2e": "pnpm --filter @juun/web exec playwright test",
    "lint-staged": "lint-staged",
    "check-types": "turbo check-types",
    "web": "pnpm --filter @juun/web",
    "api": "pnpm --filter @juun/api",
    "ui": "pnpm --filter @juun/ui",
    "db": "pnpm --filter @juun/db"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "lucide-react": "^0.548.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "sonner": "^2.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.1.0",
    "@commitlint/format": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@config/eslint": "workspace:^",
    "@storybook/addon-a11y": "^9.1.15",
    "@storybook/addon-docs": "^9.1.15",
    "@storybook/addon-links": "^9.1.15",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/coverage-v8": "^4.0.3",
    "commitizen": "^4.3.1",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "inquirer": "~9.3.8",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "storybook": "^9.1.15",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "pnpm": {
    "overrides": {
      "refractor>prismjs": "^1.30.0",
      "tmp": "^0.2.4",
      "vite": "^7.1.11"
