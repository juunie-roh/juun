{
  "name": "@juun/api",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "juun",
  "exports": {
    "./fetcher": "./src/fetcher/index.ts",
    "./react": "./src/react/index.ts",
    "./react/*": "./src/react/*.ts",
    "./openapi/types/*": "./generated/openapi/*.ts"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc -p ./tsconfig.json --noEmit",
    "generate": "node ./scripts/generate-openapi-types.cjs",
    "test": "vitest run",
    "test:watch": "vitest watch --ui --coverage"
  },
  "dependencies": {
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.90.12",
    "react": "^19.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "devDependencies": {
    "@config/eslint": "workspace:^",
    "@config/typescript": "workspace:^",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.3.0",
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
