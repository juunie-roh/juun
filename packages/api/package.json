{
  "name": "@juun/api",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "juun",
  "exports": {
    "./fetcher": {
      "types": "./dist/fetcher/index.d.ts",
      "import": "./dist/fetcher/index.js",
      "require": "./dist/fetcher/index.cjs",
      "default": "./dist/fetcher/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs",
      "default": "./dist/react/index.js"
    },
    "./openapi/*": {
      "types": "./generated/openapi/*.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc -p ./tsconfig.json --noEmit",
    "generate": "node ./scripts/generate-openapi-types.cjs",
    "test": "vitest run",
    "test:watch": "vitest watch --ui --coverage"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.90.16",
    "react": "^19.2.3",
    "zod": "^4.2.1"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    },
    "react": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "devDependencies": {
    "@config/eslint": "workspace:^",
    "@config/typescript": "workspace:^",
    "@types/node": "^25.0.10",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^27.4.0",
    "openapi-typescript": "^7.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
