{
  "name": "@juun/web",
  "version": "0.0.7",
  "private": true,
  "author": "juun",
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "postbuild": "next-sitemap",
    "build:standalone": "next build && node scripts/post-build.js",
    "check-types": "tsc -p ./tsconfig.json --noEmit",
    "dev": "next dev",
    "lint": "eslint .",
    "start": "next start",
    "start:standalone": "node .next/standalone/apps/web/server.js",
    "start:otel": "NEXT_OTEL_VERBOSE=1 OTEL_TRACES_EXPORTER=console OTEL_SERVICE_NAME=juun-web OTEL_TRACES_SAMPLER=always_on OTEL_LOG_LEVEL=debug pnpm start:standalone",
    "start:otel-log": "NEXT_OTEL_VERBOSE=1 OTEL_TRACES_EXPORTER=console OTEL_SERVICE_NAME=juun-web OTEL_TRACES_SAMPLER=always_on OTEL_LOG_LEVEL=debug pnpm start:standalone 2>&1 | tee otel-traces-$(date +%Y%m%d-%H%M%S).log",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:watch": "vitest watch --coverage"
  },
  "dependencies": {
    "@config/tailwind": "workspace:^",
    "@juun-roh/cesium-utils": "^0.3.8",
    "@juun/api": "workspace:^",
    "@juun/db": "workspace:^",
    "@juun/ui": "workspace:^",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-logs": "^0.208.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@prisma/client": "^7.1.0",
    "@react-three/cannon": "^6.6.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/analytics": "^1.6.1",
    "@vercel/otel": "^2.1.0",
    "@vercel/speed-insights": "^1.3.1",
    "cesium": "^1.136.0",
    "gray-matter": "^4.0.3",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "lucide-react": "^0.556.0",
    "next": "^16.0.7",
    "next-intl": "^4.5.8",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "probe-image-size": "^7.2.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "rehype-raw": "^7.0.0",
    "rehype-react": "^8.0.0",
    "rehype-unwrap-images": "^1.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "three": "^0.181.2",
    "unified": "^11.0.5",
    "webpack": "^5.103.0",
    "zustand": "^5.0.9"
