{
  "name": "@juun/web",
  "version": "0.0.7",
  "private": true,
  "author": "juun",
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "build:standalone": "next build && node scripts/post-build.js",
    "check-types": "tsc --noEmit --pretty",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "start:standalone": "node .next/standalone/apps/web/server.js",
    "start:otel": "NEXT_OTEL_VERBOSE=1 OTEL_TRACES_EXPORTER=console OTEL_SERVICE_NAME=juun-web OTEL_TRACES_SAMPLER=always_on OTEL_LOG_LEVEL=debug pnpm start:standalone",
    "start:otel-log": "NEXT_OTEL_VERBOSE=1 OTEL_TRACES_EXPORTER=console OTEL_SERVICE_NAME=juun-web OTEL_TRACES_SAMPLER=always_on OTEL_LOG_LEVEL=debug pnpm start:standalone 2>&1 | tee otel-traces-$(date +%Y%m%d-%H%M%S).log",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:watch": "vitest watch --coverage"
  },
  "dependencies": {
    "@config/tailwind": "workspace:^",
    "@juun-roh/cesium-utils": "^0.3.3",
    "@juun/ui": "workspace:^",
    "@opentelemetry/api": "^1.9.0",
    "@react-three/cannon": "^6.6.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/otel": "^1.13.0",
    "@vercel/speed-insights": "^1.2.0",
    "cesium": "^1.134.0",
    "gray-matter": "^4.0.3",
    "marked": "^16.4.0",
    "next": "^15.5.4",
    "next-intl": "^4.3.9",
    "three": "^0.180.0",
    "webpack": "^5.102.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@config/eslint": "workspace:^",
    "@config/typescript": "workspace:^",
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.56.0",
    "@storybook/nextjs": "^9.1.10",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^24.7.0",
    "@types/three": "^0.180.0",
    "ts-node": "^10.9.2"
  }
}
