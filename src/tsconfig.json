{
  "extends": "@juun/tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": "../",
    "rootDir": "../",
    "paths": { "@/*": ["./*"] }
  },
  "include": [
    "next-env.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
    ".next/types/**/*.ts",
    "./**/*.js",
    "./**/*.jsx"
  ],
  "exclude": ["node_modules", "out"]
}
